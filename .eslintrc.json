{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/no-multi-comp": 0,
    "consistent-return": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,
    "import/first": 0,
    "react/sort-comp": 0
  }
}
